EVENTNAME 0;Head in the Wall

STEP 1;
OPT focuscam 1;
OPT hud 0;
OPT uke 0;
OPT tori 1;
KEYBOARDLOCK;
JOINTLOCK;
CUSTOMFUNC PrepareNewGame;
LOADPLAYER 0 PLAYER;

STEP 2;
CUSTOMFUNC IntroOverlay;

STEP 3;
SHOWSAYMESSAGE;
SAY KAREN;
MESSAGE INTRO1;
WAITBUTTON;

STEP 4;
SAY KAREN;
MESSAGE INTRO2;
WAITBUTTON;

STEP 5;
SAY KAREN;
MESSAGE INTRO3;
WAITBUTTON;

STEP 6;
HIDESAYMESSAGE;
SHOWTASKMESSAGE;
TASK HWTASK1;
TASKADD 1	HWTASKADD1;
HIDESAYMESSAGE;
HIDEWAITBUTTON;
STEPSKIP 4;

STEP 7;
CUSTOMFUNC IntroOverlay;

STEP 8;
SHOWSAYMESSAGE;
SAY KAREN;
MESSAGE INTRO4;
WAITBUTTON;

STEP 9;
SHOWTASKMESSAGE;
TASK HWTASK1;
TASKADD 1	HWTASKADD1;
HIDESAYMESSAGE;
HIDEWAITBUTTON;

STEP 10;
OPT hud 1;
-OPT chat 0;
OPT score 0;
OPT text 0;
KEYBOARDUNLOCK WASD;
SHIFTUNLOCK;
CUSTOMFUNC LoadReplay;

STEP 11;
OPT hud 1;
-OPT chat 0;
OPT score 0;
OPT text 0;
DELAY 0;
KEYBOARDUNLOCK WASDZXCVPREBGL12F ;
SHIFTUNLOCK;
JOINTUNLOCK;

STEP 12;
CUSTOMFUNC HeadInTheWall;

STEP 13;
OPT hud 0;
KEYBOARDLOCK;
JOINTLOCK;
CUSTOMFUNC PlayerWon;
STEPFALLBACK 2;

STEP 14;
OPT hud 0;
KEYBOARDLOCK;
JOINTLOCK;
CUSTOMFUNC PlayerLost;
STEPFALLBACK 3;

STEP 15;
SHOWWAITBUTTON;
HIDETASKMESSAGE;
SHOWSAYMESSAGE;
SAY KAREN;
MESSAGE UPLOADSUCCESS1;
WAITBUTTON;

STEP 16;
SAY KAREN;
MESSAGE UPLOADSUCCESS2;
WAITBUTTON;

STEP 17;
SAY KAREN;
MESSAGE UPLOADSUCCESS3;
WAITBUTTON;

STEP 18;
SAY KAREN;
MESSAGE UPLOADSUCCESS4;
WAITBUTTON;

STEP 19;
HIDESAYMESSAGE;
CUSTOMFUNC OutroOverlay;
